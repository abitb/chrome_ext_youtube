{
	"manifest_version": 2,
	"name": "Youtube Quality",
	"version": "0.1",
	"description": "",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts" : [{
        "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
        "js" : ["content_script.js"]
    }],
    "web_accessible_resources": ["inject.js"] ,
    "permissions": ["unlimitedStorage","http://www.google.com/"]
}